{
  "title": "YiSan Development Documentation",
  "description": "YiSan 프로젝트 개발 문서 - DevLog, Planning, System Review",
  "author": "DoppledDiggong Team",
  "language": "ko",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-comscore",
    "github",
    "collapsible-menu",
    "back-to-top-button",
    "search-plus",
    "expandable-chapters-small",
    "page-toc-button",
    "-anchor-navigation-ex",
    "code",
    "alerts",
    "emphasize",
    "klipse",
    "-lunr",
    "-search",
    "-sharing"
  ],
  "pluginsConfig": {
    "theme-comscore": {
      "showLevel": true,
      "logo": "Reference/icon_yisan.png",
      "favicon": "Reference/icon_yisan512.png"
    },
    "github": {
      "url": "https://github.com/{{GITHUB_USER}}/{{REPO_NAME}}"
    },
    "page-toc-button": {
      "maxTocDepth": 3,
      "minTocSize": 2
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "float": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "emphasize": {
      "style": "yellow"
    },
    "alerts": {
      "info": {
        "label": "정보",
        "icon": "fa fa-info-circle",
        "className": "info"
      },
      "warning": {
        "label": "주의",
        "icon": "fa fa-exclamation-triangle",
        "className": "warning"
      },
      "danger": {
        "label": "위험",
        "icon": "fa fa-times-circle",
        "className": "danger"
      },
      "tip": {
        "label": "팁",
        "icon": "fa fa-lightbulb-o",
        "className": "tip"
      }
    }
  },
  "links": {
    "sidebar": {
      "GitHub Repository": "https://github.com/{{GITHUB_USER}}/{{REPO_NAME}}",
      "Doxygen API Docs": "https://{{GITHUB_USER}}.github.io/{{REPO_NAME}}/doxygen/"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
